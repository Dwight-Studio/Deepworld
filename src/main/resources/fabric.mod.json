{
  "schemaVersion": 1,
  "id": "deepworld",
  "version": "${version}",

  "name": "Deepworld",
  "description": "To make the nether go truly deep. The Deep World is a mod that introduces a new intermediate dimension between the overworld and the nether to complexify the access to the latter.",
  "authors": [
    "Deleranax, GamerMine, Yinx"
  ],
  "contact": {
    "homepage": "http://dpt.dwight-studio.fr",
    "sources": ""
  },

  "license": "CC0-1.0",
  "icon": "assets/deepworld/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "fr.dwightstudio.deepworld.Deepworld"
    ],"client": [
      "fr.dwightstudio.deepworld.DeepworldClient"
    ],
    "fabric-datagen": [
      "fr.dwightstudio.deepworld.DeepworldDataGenerator"
    ]
  },
  "mixins": [
    "deepworld.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.14",
    "fabric-api": "*",
    "minecraft": "~1.19.3",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
